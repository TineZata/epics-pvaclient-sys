record(ao, "TEST:PV_Double") {
    field(DESC, "Output test PV Double")
    field(DTYP, "Soft Channel")
    field(VAL, "87654.3210123")
    field(EGU, "um")
    field(PREC, "5")
    # JSON style metadata for grouping
    info(Q:group, {
        "TEST:GROUP1": { 
            "Alias_Double": {"+channel": "VAL"}
        }
    })
}

